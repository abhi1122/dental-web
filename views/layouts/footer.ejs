    <!-- footer -->
    <footer class="py-5">
        <div class="container py-sm-3">
            <div class="row footer-grids">
                <div class="col-lg-5 col-sm-6 mb-lg-0 mb-sm-5 mb-4">
                    <h4 class="mb-sm-4 mb-3"><%= addressText %></h4>
                    <p style="width: 250px;"><span class="fa mr-2 fa-map-marker"></span>
                        <%= address %></p>
                    <p class="phone py-2"><span class="fa mr-2 fa-phone"></span>
                        <%= phone1 %> <%= phone1 %> </p>
                    <p><span class="fa mr-2 fa-envelope"></span><a href="mailto:<%= email %>"><%= email %></a>
                    </p>
                </div>
                <div class="col-lg-4 col-sm-6 mb-sm-0 mb-4">
                    <h4 class="mb-sm-4 mb-3"><%= quickLinks %></h4>
                    <ul>
                        <% 
                        for (var i = 0; i < menu.length; i++) {
                        %>
                        <li><a href="<%= menu[i]['url'] %>"><%= menu[i]['text'] %></a></li>
                        <% } %>
                    </ul>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <h4 class="mb-sm-4 mb-3"><%= followUsText %></h4>

                    <div class="icon-social mt-3">
                        <% 
                        for (var i = 0; i < follow.length; i++) {
                        %>
                        <a href="<%= follow[i]['url'] %>" class="button-footr">
                            <span class="<%= follow[i]['class'] %>"></span>
                        </a>
                        <% } %>
                    </div>
                </div>
            </div>
        </div>
    </footer>
