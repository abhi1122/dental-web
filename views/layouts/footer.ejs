    <!-- appointment -->
    <section class="appointment text-center py-5">
        <div class="container py-sm-3">
            <h4><%= tagLine %></h4>
            <p class="mt-3"><%= callUs %> <b style="font-size: 18px;"><%= phone1 %> <%= phone2 %></b>
                <%= orBook %></p>
